     */
    where: SearchTrendWhereUniqueInput
  }

  /**
   * SearchTrend deleteMany
   */
  export type SearchTrendDeleteManyArgs<ExtArgs extends $Extensions.InternalArgs = $Extensions.DefaultArgs> = {
    /**
     * Filter which SearchTrends to delete
     */
    where?: SearchTrendWhereInput
  }

  /**
   * SearchTrend without action
   */
  export type SearchTrendDefaultArgs<ExtArgs extends $Extensions.InternalArgs = $Extensions.DefaultArgs> = {
    /**
     * Select specific fields to fetch from the SearchTrend
     */
    select?: SearchTrendSelect<ExtArgs> | null
  }


