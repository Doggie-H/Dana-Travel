    findUniqueOrThrow<T extends ChatLogFindUniqueOrThrowArgs>(args: SelectSubset<T, ChatLogFindUniqueOrThrowArgs<ExtArgs>>): Prisma__ChatLogClient<$Result.GetResult<Prisma.$ChatLogPayload<ExtArgs>, T, "findUniqueOrThrow">, never, ExtArgs>

    /**
