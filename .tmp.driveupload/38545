  export type GetSearchTrendAggregateType<T extends SearchTrendAggregateArgs> = {
        [P in keyof T & keyof AggregateSearchTrend]: P extends '_count' | 'count'
      ? T[P] extends true
        ? number
        : GetScalarType<T[P], AggregateSearchTrend[P]>
      : GetScalarType<T[P], AggregateSearchTrend[P]>
  }




  export type SearchTrendGroupByArgs<ExtArgs extends $Extensions.InternalArgs = $Extensions.DefaultArgs> = {
    where?: SearchTrendWhereInput
    orderBy?: SearchTrendOrderByWithAggregationInput | SearchTrendOrderByWithAggregationInput[]
    by: SearchTrendScalarFieldEnum[] | SearchTrendScalarFieldEnum
    having?: SearchTrendScalarWhereWithAggregatesInput
    take?: number
    skip?: number
    _count?: SearchTrendCountAggregateInputType | true
    _avg?: SearchTrendAvgAggregateInputType
    _sum?: SearchTrendSumAggregateInputType
    _min?: SearchTrendMinAggregateInputType
    _max?: SearchTrendMaxAggregateInputType
  }

