    id: string
    tags: string
    duration: string | null
    budget: number | null
    people: number | null
    createdAt: Date
    _count: SearchTrendCountAggregateOutputType | null
    _avg: SearchTrendAvgAggregateOutputType | null
    _sum: SearchTrendSumAggregateOutputType | null
    _min: SearchTrendMinAggregateOutputType | null
    _max: SearchTrendMaxAggregateOutputType | null
  }

  type GetSearchTrendGroupByPayload<T extends SearchTrendGroupByArgs> = Prisma.PrismaPromise<
    Array<
      PickEnumerable<SearchTrendGroupByOutputType, T['by']> &
        {
          [P in ((keyof T) & (keyof SearchTrendGroupByOutputType))]: P extends '_count'
            ? T[P] extends boolean
              ? number
              : GetScalarType<T[P], SearchTrendGroupByOutputType[P]>
            : GetScalarType<T[P], SearchTrendGroupByOutputType[P]>
        }
      >
    >


  export type SearchTrendSelect<ExtArgs extends $Extensions.InternalArgs = $Extensions.DefaultArgs> = $Extensions.GetSelect<{
    id?: boolean
    tags?: boolean
    duration?: boolean
    budget?: boolean
    people?: boolean
    createdAt?: boolean
  }, ExtArgs["result"]["searchTrend"]>

